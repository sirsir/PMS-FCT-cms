<%
col_field = @report_request.report.col_fields_reports.first
if col_field
  rrc_groups = @report_request.col_report_request_cols(col_field)
%>
  <%=render :partial => 'report_request_cols', :collection => rrc_groups[0] %>
  <% if @report_request.report.col_fields_reports.first == col_field &&
      @report_request.report.display_grand_totals.include?(:col)
    cell_field_report_count = (@report_request.report.cell_location == :row) ? 1 : @report_request.report.cell_fields_reports.size
  %>
  <%=render :partial => "sub_total_cell_field", :collection => @report_request.report.cell_fields_reports[0..cell_field_report_count-1] %>
  <% end %>
<% end %>