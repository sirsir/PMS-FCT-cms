<tr>
  <%
  field_type_optional_tag = left_to_right_tag(
    :descr => "Option",
    :name =>  "custom_field[value][label_ids]",
    :assigned_options => @assigned_options,
    :all_options => @labels,
    :value_method => "id",
    :text_method => "descr")
%>
  <th class="th_row_header">ComboBox Options:</th>
  <td>
    <%=field_type_optional_tag %>
    <%= field_descr_tag %>
  </td>
</tr>
<tr>
  <th class="th_row_header">Default Value:</th>
  <td>
    <%= select_label_tag("custom_field[value][default_value]", @custom_field.default_value.to_i) %>
    <%= field_descr_tag %>
  </td>
</tr>