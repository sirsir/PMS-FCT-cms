<tr class="tr_header" ><th class="group_caption_center" colspan="2"><%= "#{filter} memory".titleize %></th></tr>
<tr class="tr_header"><th class="th_col_header">Name</th><th class="th_col_header">Value</th></tr>
<%
  b = true
  @mem_stat.system_memory.select{|s| s.inspect =~ /#{filter}/i}.each do |m|
%>
<tr class="tr_data_<%= (b ^= true) ? 'odd' : 'even' %>">
  <%
  value = m.last
  m.delete(value)
  name = m.join(' ').gsub(/#{filter} memory/i, '').titleize
  %>
  <td class="td_cell"><%= name %></td>
  <td class="td_cell" style="text-align: right;"><%= value %></td>
</tr>
<% end %>
